{
    "name": "zzzzzqs/repayment",
    "description": "A php package for repayment calculation",
    "license": "MIT",
    "authors": [
        {
            "name": "zzzzzqs",
            "email": "forwsp2019@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.42"
    },
    "autoload": {
        "psr-4": {
            "Zzzzzqs\\Repayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zzzzzqs\\Repayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zzzzzqs\\Repayment\\RepaymentServiceProvider"
            ],
            "aliases": {
                "Repayment": "Zzzzzqs\\Repayment\\Facades\\Repayment"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=config --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=config --ansi"
        ]
    }
}
